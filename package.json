{
  "name": "project-jarvis",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "pnpm -C apps/backend dev",
    "dev:mobile": "pnpm -C apps/mobile start",
    "dev:mcp": "pnpm -C apps/mcp_servers dev:unified",
    "start:mcp": "pnpm -C apps/mcp_servers start:unified",
    "build:mcp": "pnpm -C apps/mcp_servers build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "dependencies": {
    "expo": "^54.0.0",
    "react-dom": "19.1.0",
    "react-native-web": "^0.21.0"
  }
}
